{
  "name": "bibliokit-simple",
  "version": "1.0.0",
  "description": "BiblioKit - Simple Launch Version",
  "type": "module",
  "scripts": {
    "dev": "vite --port 9990",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview --port 9990",
    "serve": "vite preview --port 9990",
    "netlify:dev": "netlify dev",
    "netlify:build": "npm run build"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.303.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@netlify/functions": "^2.6.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "netlify-cli": "^17.15.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
